<?xml version="1.0"?>
<launch>
  <arg name="marker_size" default="18.23" />
  <arg name="max_new_marker_error" default="0.08" />
  <arg name="max_track_error" default="0.2" />
  <arg name="cam_image_topic" default="/camera/rgb/image_rect_color" />
  <arg name="cam_info_topic" default="/camera/rgb/camera_info" />		
  <arg name="output_frame" default="waist_link" />
  <arg name="node_name" default="ar_tracker_alvar" />
  <arg name="tf_prefix" default="test"/>

  <node name="$(arg node_name)" pkg="vizzy_calibration" type="markersNoKinect" respawn="false" args="$(arg marker_size) $(arg max_new_marker_error) $(arg max_track_error) $(arg cam_image_topic) $(arg cam_info_topic) $(arg output_frame)  $(arg tf_prefix)">
  </node>
</launch>
