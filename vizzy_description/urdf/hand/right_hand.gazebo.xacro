			<plugin name="mimic_plugin" filename="libgazebo_mimic_plugin.so">
				<joint>r_thumb_phal_2_joint</joint>
				<mimicJoint>r_thumb_phal_4_joint</mimicJoint>
				<multiplier>0.5</multiplier>
			</plugin>
		</gazebo>

  </xacro:macro>

</robot> 


