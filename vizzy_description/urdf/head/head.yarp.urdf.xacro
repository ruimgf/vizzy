        rpy="0.0 0.0 ${-M_PI/2}" />
      <mass
        value="0.223143594366725" />
      <inertia
        ixx="0.000359045077001746"
        ixy="3.18553746848725E-08"
        ixz="-1.33740307483815E-05"
        iyy="0.000368715218013968"
        iyz="-4.52681716726929E-07"
        izz="8.59048961267353E-05" />
